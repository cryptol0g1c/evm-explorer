{
  "name": "bitsign-explorer",
  "version": "0.0.9",
  "scripts": {
    "start": "node server.js",
    "devmode": "rm -rf ./dist && npm-run-all --parallel start:client start:server",
    "start:client": "rm -rf ./dist && webpack --config webpack.config.client.js",
    "start:server": "nodemon server.js --ignore 'frontend/' --ignore 'dist/'",
    "build": "rm -rf dist/ && cross-env BABEL_ENV=production ./node_modules/.bin/webpack --config webpack.config.production.js",
    "lint": "./node_modules/.bin/eslint --ext .js routes",
    "chain:build": "sh chain/chain_build.sh",
    "chain:start": "sh chain/chain_start.sh",
    "chain:stop": "docker stop bitsign-federation-devs"
  },
  "dependencies": {
    "@parity/api": "^2.1.24",
    "antd": "^3.10.4",
    "bignumber.js": "^8.0.1",
    "bluebird": "^3.5.1",
    "bytecode-utils": "^1.0.1",
    "cookie-parser": "1.4.3",
    "ethereum-input-data-decoder": "0.0.12",
    "ethereumjs-abi": "^0.6.5",
    "ethereumjs-tx": "^1.3.7",
    "ethereumjs-util": "^5.2.0",
    "exec": "^0.2.1",
    "express": "4.16.2",
    "husky": "^1.2.0",
    "lodash": "^4.17.4",
    "moment": "^2.22.2",
    "mongoose": "^5.2.6",
    "nodemon": "^1.18.4",
    "numeral": "^2.0.6",
    "react": "^16.6.3",
    "react-alert": "^4.0.4",
    "react-alert-template-basic": "^1.0.0",
    "react-copy-to-clipboard": "^5.0.1",
    "react-dom": "^16.3.2",
    "react-eth": "^0.2.2",
    "react-redux": "^5.0.7",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2",
    "react-spinkit": "^3.0.0",
    "react-syntax-highlighter": "^8.0.1",
    "react-transition-group": "^2.5.0",
    "redux": "^3.7.2",
    "redux-devtools-extension": "^2.13.5",
    "redux-thunk": "^2.3.0",
    "socket.io": "^2.1.1",
    "solc": "^0.4.16",
    "tmp": "^0.0.33",
    "web3": "^1.0.0-beta.35",
    "ws": "^6.0.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-eslint": "^9.0.0",
    "babel-loader": "^6.4.1",
    "babel-polyfill": "^6.26.0",
    "babel-preset-env": "^1.1.10",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "clean-webpack-plugin": "^0.1.19",
    "copy-webpack-plugin": "^4.5.2",
    "cross-env": "^5.0.5",
    "css-loader": "^0.28.7",
    "eslint": "^5.4.0",
    "eslint-plugin-babel": "^5.1.0",
    "eslint-plugin-react": "^7.11.1",
    "extract-text-webpack-plugin": "^3.0.1",
    "file-loader": "^1.1.5",
    "font-awesome": "^4.7.0",
    "foundation-sites": "^6.4.4-rc1",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.5.3",
    "npm-run-all": "^4.0.2",
    "postcss-loader": "^2.0.7",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.19.0",
    "webpack": "^3.7.1",
    "webpack-livereload-plugin": "^1.2.0",
    "webpack-node-externals": "^1.5.4"
  },
  "husky": {
    "hooks": {
      "pre-commit": "npm run lint"
    }
  }
}
